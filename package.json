{
  "name": "website",
  "version": "0.9.0",
  "private": true,
  "description": "Personal site built with React, Next.js, and Material Components",
  "bugs": "https://github.com/4cm4k1/website/issues",
  "repository": "https://github.com/4cm4k1/website",
  "license": "MIT",
  "author": "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>",
  "contributors": [
    "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>"
  ],
  "scripts": {
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "build": "next build",
    "dev": "next",
    "gen-dev-certs": "mkcert localhost 127.0.0.1 ::1 && mv localhost+2.pem cert.pem && mv localhost+2-key.pem key.pem",
    "now-build": "next build --lambdas",
    "start": "next start",
    "prettier": "prettier '{*,{components,scss,mdx,pages}/**/*}.{babelrc,scss,js,json,jsx,md,mdx}' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "next": "^7.0.2-canary.29",
    "next-server": "^7.0.2-canary.29",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2"
  },
  "devDependencies": {
    "@material/react-drawer": "^0.6.2",
    "@material/react-layout-grid": "^0.7.0",
    "@material/react-material-icon": "^0.7.0",
    "@material/react-top-app-bar": "^0.7.0",
    "@mdx-js/loader": "^0.16.5",
    "@mdx-js/mdx": "^0.16.5",
    "@rmwc/list": "^4.0.0",
    "@types/material-components-web": "^0.35.0",
    "@types/next": "^7.0.5",
    "@types/next-server": "^7.0.2",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@zeit/next-bundle-analyzer": "^0.1.3-canary.1",
    "@zeit/next-mdx": "^1.2.1-canary.0",
    "@zeit/next-sass": "^1.0.2-canary.2",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "autotrack": "^2.4.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "fibers": "^3.1.1",
    "husky": "^1.2.0",
    "modern-normalize": "^0.5.0",
    "next-compose-plugins": "^2.1.1",
    "next-offline": "^3.2.1",
    "next-purgecss": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "purgecss-webpack-plugin": "^1.3.1",
    "sass": "^1.15.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "renovate-config": {
    "assignees": [
      "4cm4k1"
    ],
    "extends": [
      "config:base",
      ":pinAllExceptPeerDependencies"
    ],
    "labels": [
      "renovate"
    ]
  }
}
